		"@types/react-dom": "^19.1.6",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@vitejs/plugin-react": "^4.6.0",
		"@vitest/ui": "^3.2.4",
		"concurrently": "^9.2.1",
		"eslint": "^9.29.0",
